parameters:
    level: 8
    paths:
        - bin/
        - config/
        - public/
        - src/
        - tests/
    checkGenericClassInNonGenericObjectType: false
    ignoreErrors:
        - '#Call to an undefined method [a-zA-Z0-9\\_\<\>]+::findOneBy[A-Z][a-zA-Z]*\(\)\.#'
        - '#Call to an undefined method [a-zA-Z0-9\\_\<\>]+::getAll\(\)\.#'